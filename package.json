{
  "name": "unplugin-parcel-css",
  "version": "0.0.0-development",
  "packageManager": "pnpm@7.2.1",
  "description": "@parcel/css plugin for vite/rollup/webpack",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "parcel-css"
  ],
  "homepage": "https://github.com/ssssota/unplugin-parcel-css#readme",
  "bugs": {
    "url": "https://github.com/ssssota/unplugin-parcel-css/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ssssota/unplugin-parcel-css.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "prettier --check . & eslint --ignore-path .prettierignore .",
    "fmt": "prettier --write .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "fixtures": "esno scripts/pretest.ts",
    "test": "npm run fixtures && vitest",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@parcel/css": "^1.12.0",
    "unplugin": "^0.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/node": "^18.0.6",
    "bumpp": "^8.2.1",
    "css-loader": "^6.7.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.11",
    "husky": "^8.0.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.0",
    "rollup-plugin-css-only": "^3.1.0",
    "semantic-release": "^19.0.3",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vitest": "^0.18.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
